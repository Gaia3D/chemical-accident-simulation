<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gaia3d.casapp.repository.AccidentRepository">

    <select id="getByAccidentNo" resultType="AccidentInfo">
        select *
        from accident
        where accident_no = #{accidentNo}
    </select>

</mapper>